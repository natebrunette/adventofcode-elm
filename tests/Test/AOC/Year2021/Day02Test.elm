module Test.AOC.Year2021.Day02Test exposing (suite)

import AOC.Year2021.Day02 exposing (part1, part2)
import Expect
import List.Extra
import Test exposing (Test, describe, test)


suite : Test
suite =
    describe "AOC 2021 - Day 02"
        [ test "part 1 sample" <|
            \_ ->
                Expect.equal "150" (part1 (sample |> toList))
        , test "part 2 sample" <|
            \_ ->
                Expect.equal "900" (part2 (sample |> toList))
        , test "part 1" <|
            \_ ->
                Expect.equal "2073315" (part1 (input |> toList))
        , test "part 2" <|
            \_ ->
                Expect.equal "1840311528" (part2 (input |> toList))
        ]


toList : String -> List String
toList string =
    string
        |> String.split "\n"
        |> List.map String.trim
        |> List.drop 1
        |> List.Extra.init
        |> Maybe.withDefault []


