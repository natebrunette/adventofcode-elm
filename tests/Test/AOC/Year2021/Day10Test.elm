module Test.AOC.Year2021.Day10Test exposing (suite)

import AOC.Year2021.Day10 exposing (part1, part2)
import Expect
import List.Extra
import Test exposing (Test, describe, test)


suite : Test
suite =
    describe "AOC 2021 - Day 10"
        [ test "part 1 sample" <|
            \_ ->
                Expect.equal "26397" (part1 (sample |> toList))
        , test "part 2 sample" <|
            \_ ->
                Expect.equal "288957" (part2 (sample |> toList))
        , test "part 1" <|
            \_ ->
                Expect.equal "392367" (part1 (input |> toList))
        , test "part 2" <|
            \_ ->
                Expect.equal "2192104158" (part2 (input |> toList))
        ]


toList : String -> List String
toList string =
    string
        |> String.split "\n"
        |> List.map String.trim
        |> List.drop 1
        |> List.Extra.init
        |> Maybe.withDefault []


